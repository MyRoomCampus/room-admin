{
    "name": "room-admin",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint:script": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src",
        "lint:style": "stylelint --fix --custom-syntax postcss-less \"src/**/*.{css,less}\"",
        "lint": "npm run lint:script && npm run lint:style",
        "postinstall": "husky install",
        "plop": "plop"
    },
    "lint-staged": {
        "**/*.{js,jsx,tsx,ts}": [
            "npm run lint:script",
            "git add ."
        ],
        "**/*.{scss,less}": [
            "npm run lint:style",
            "git add ."
        ]
    },
    "dependencies": {
        "@douyinfe/semi-icons": "^2.10.2",
        "@douyinfe/semi-ui": "^2.10.1",
        "@microsoft/signalr": "^6.0.5",
        "@types/lodash-es": "^4.17.6",
        "ahooks": "^3.4.0",
        "axios": "^0.27.2",
        "buffer": "^6.0.3",
        "eslint-config-standard-with-typescript": "^21.0.1",
        "lodash-es": "^4.17.21",
        "moment": "^2.29.3",
        "postcss": "^8.3.3",
        "react": "^18.0.0",
        "react-color": "^2.19.3",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.0.0",
        "react-json-view": "^1.21.3",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "reset-css": "^5.0.1",
        "rollup": "^2.60.0"
    },
    "devDependencies": {
        "@amatlash/vite-plugin-stylelint": "^1.2.0",
        "@commitlint/cli": "^16.2.4",
        "@commitlint/config-conventional": "^16.2.4",
        "@originjs/vite-plugin-federation": "^1.1.6",
        "@types/lodash": "^4.14.182",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "@vitejs/plugin-react": "^1.3.0",
        "commitlint": "^16.2.4",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.29.4",
        "husky": "^8.0.1",
        "less": "^4.1.2",
        "lint-staged": "^12.4.1",
        "plop": "^3.1.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.6.2",
        "rollup-plugin-visualizer": "^5.6.0",
        "stylelint": "^14.8.2",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-prettier": "^2.0.0",
        "typescript": "^4.6.3",
        "typescript-plugin-css-modules": "^3.4.0",
        "vite": "^2.9.7",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-eslint": "^1.6.0",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-imagemin": "^0.6.1",
        "vite-plugin-top-level-await": "^1.0.0"
    }
